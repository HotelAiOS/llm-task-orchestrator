{
    "models": {
        "code_generation": {
            "model": "deepseek-coder:latest",
            "temperature": 0.3,
            "max_tokens": 2048,
            "description": "Specjalistyczny model do generowania kodu"
        },
        "text_analysis": {
            "model": "llama3.2:latest",
            "temperature": 0.5,
            "max_tokens": 2048,
            "description": "Model do analizy i przetwarzania tekstu"
        },
        "translation": {
            "model": "llama3.2:latest",
            "temperature": 0.3,
            "max_tokens": 2048,
            "description": "Model do tłumaczeń"
        },
        "summarization": {
            "model": "llama3.2:latest",
            "temperature": 0.4,
            "max_tokens": 1024,
            "description": "Model do tworzenia streszczeń"
        },
        "qa": {
            "model": "llama3.2:latest",
            "temperature": 0.7,
            "max_tokens": 2048,
            "description": "Model do odpowiadania na pytania"
        },
        "creative": {
            "model": "llama3.2:latest",
            "temperature": 0.9,
            "max_tokens": 4096,
            "description": "Model do zadań kreatywnych"
        },
        "math": {
            "model": "qwen2.5:latest",
            "temperature": 0.1,
            "max_tokens": 2048,
            "description": "Model specjalizujący się w matematyce"
        },
        "extraction": {
            "model": "llama3.2:latest",
            "temperature": 0.2,
            "max_tokens": 1024,
            "description": "Model do ekstrakcji informacji"
        }
    },
    "decomposer": {
        "model": "llama3.2:latest",
        "temperature": 0.5,
        "description": "Model używany do dekompozycji zadań"
    },
    "merger": {
        "model": "llama3.2:latest",
        "temperature": 0.6,
        "description": "Model używany do łączenia wyników"
    },
    "embedding": {
        "model": "nomic-embed-text:latest",
        "description": "Model do tworzenia embeddingów dla bazy wektorowej"
    },
    "knowledge_base": {
        "collection_name": "knowledge_base",
        "chunk_size": 500,
        "chunk_overlap": 50,
        "search_results": 3
    },
    "api": {
        "host": "0.0.0.0",
        "port": 8000,
        "reload": true,
        "workers": 4
    },
    "ollama": {
        "host": "http://ollama:11434",
        "timeout": 120
    },
    "chromadb": {
        "host": "http://chromadb:8000",
        "persist_directory": "./vector_db"
    }
}
